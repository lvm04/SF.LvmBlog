@model AuthModel

<ul class="navbar-nav">
@if (Model.Login != null)
{
    <li class="nav-item">
        <a  class="nav-link text-dark" asp-action="Manage" asp-controller="Account">[ @Model.Name (@Model.Login) ]</a>
    </li>
    <li class="nav-item">
        <a class="nav-link text-dark" asp-action="Logout" asp-controller="Account">Выйти</a>
    </li>
}
else
{
    <li class="nav-item">
        <a class="nav-link text-dark" asp-action="Register" asp-controller="Account">Регистрация</a>
    </li>
    <li class="nav-item">
        <a class="nav-link text-dark" asp-action="Login" asp-controller="Account">Войти</a>
    </li>
}
</ul>